<%= render "shared/admin_nav" %>

<h1>New Image</h1>

<%= form_for :image, url: images_path do |f| %>
  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :url %><br>
    <%= f.text_area :url %>
  </p>

  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>

  <p>
    <%= f.label :picture %><br>
    <%= f.file_field :picture %>
  </p>

  <p>
    <%= f.submit 'Save'%>
  </p>
<% end %>
<br>
<br>
<%= link_to 'Back', images_path %>
